[
  {
    "description":"模拟一个没有参数的post请求",
    "request":{
      "uri":"/postdemo",
      "method":"post"
    },
    "response":{
      "headers":{"Content-Type":"text/html;charset=utf8"},
      "text":"第一个mock的post请求返回报文"
    }
  },
  {
  "description":"模拟一个带参数-key-value方式的post请求",
  "request":{
    "uri":"/postwithparm",
    "method":"post",
    "forms":{
      "name":"fengjm",
      "age":"30"
    }
  },
  "response":{
    "headers":{"Content-Type":"text/html;charset=utf8"},
    "text":"第一个mock的带参数-key-value方式的post请求返回报文"
  }
},
  {
    "description":"模拟一个带参数-json方式的post请求",
    "request":{
      "uri":"/postwithparm",
      "method":"post",
      "json":{
        "name":"fengjm",
        "age":"30"
      }
    },
    "response":{
      "headers":{"Content-Type":"text/html;charset=utf8"},
      "text":"第一个mock的带参数-json方式的post请求返回报文"
    }
  },
  {
    "description":"模拟一个带cookies的post请求",
    "request":{
      "uri":"/post/with/cookies",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"fengjm",
        "age":"30"
      }
    },
    "response":{
        "status":200,
        "json":{
          "fengjm":"success",
          "status":"1"
        }
        }
  },
  {
    "description":"模拟一个带header的post请求",
    "request":{
      "uri":"/post/with/headers",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "headers":{
        "content-type":"application/json"
      },
      "json":{
        "name":"fengjm",
        "age":"30"
      }
    },
    "response":{
      "status":200,
      "json":{
        "fengjm":"success",
        "status":"1"
      }
    }
  },
  {
    "description":"这个是一个返回cookies的post请求",
    "request":{
      "uri":"/postCookies",
      "method":"post"
    },
    "response":{
      "cookies":{
        "login":"true"
      },
      "text":"恭喜你获取cookies成功"
    }
  }




]